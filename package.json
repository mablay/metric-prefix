{
  "name": "metric-prefix",
  "version": "0.9.3",
  "description": "Stringify big.js numbers using only few significant digits and a metric prefix",
  "main": "index.js",
  "scripts": {
    "test": "pta",
    "build": "npx webpack-cli --mode production --entry ./doc/script.js -t web -o doc/",
    "watch": "npx webpack-cli --mode development --entry ./doc/script.js -t web -o doc/ --watch"
  },
  "keywords": [
    "metric",
    "si",
    "prefix",
    "unit",
    "kilo",
    "mega",
    "giga",
    "terra",
    "milli",
    "micro",
    "significant",
    "number",
    "big",
    "big.js",
    "digits",
    "decimals"
  ],
  "author": "Marc Ablay",
  "license": "ISC",
  "dependencies": {
    "big.js": "^5.1.2"
  },
  "devDependencies": {
    "pta": "^0.1.3"
  }
}
